{
  "name": "bloglist-backend",
  "version": "1.0.0",
  "description": "Blog List Application",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "watch": "nodemon index.js",
    "lint": "eslint .",
    "build:ui": "rm -rf build && cd ../../part5/bloglist-frontend/ && npm run build --prod && cp -r build ../../part5/bloglist-backend/",
    "deploy": "git push heroku master",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "test": "jest --verbose"
  },
  "keywords": [
    "ReactJs",
    "express",
    "mongodb",
    "mongoose",
    "html",
    "css",
    "env",
    "nodemon",
    "cors"
  ],
  "author": "Medi Risasi",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.8.6",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
}
